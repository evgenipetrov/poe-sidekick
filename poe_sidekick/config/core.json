{
    "window": {
        "title": "Path of Exile 2",
        "executable": "PathOfExile.exe",
        "capture_rate": 1,
        "detection_timeout": 30.0,
        "detection_interval": 1.0
    },
    "input": {
        "min_delay_seconds": 0.1,
        "max_delay_seconds": 0.3,
        "failsafe_enabled": true,
        "failsafe_points": [[0, 0]]
    },
    "engine": {
        "shutdown_timeout": 5.0,
        "module_init_timeout": 3.0
    },
    "screenshot_stream": {
        "metrics": {
            "frame_time_window": 100,
            "memory_window": 100,
            "processing_window": 100,
            "metrics_save_interval": 300,
            "debug_frame_interval": 5
        },
        "performance": {
            "target_fps": 0.5,
            "frame_buffer_size": 10,
            "max_memory_mb": 500,
            "max_processing_ms": 50
        }
    }
}
